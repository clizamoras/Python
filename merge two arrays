
class Solution:
  def merge(self,num1,num2):
    i=0
    j=0
    list=[]
    while(i<len(num) and j<len(num2)):
       if(num1[i]>num2[j]):
          list.append(num1[i])
          i=i+1
       elif(num1[i]>num2[j):
          list.append(num2[j])
          j=j+1
      else:
         list.append(num1[i])
         i=i+1
         j=j+1
    list.extend(num1[i:])
    list.extend(num2[j:])
    return list
num1=[-5, -2, 4, 5, 0, 0, 0]
num2=[-3, 1, 8]
obj=Solution()
print(obj.merge(num1,num2))
