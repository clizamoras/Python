

class Solution:
  def lead(self,arr):
     result=[]
     n=len(arr)
     for i in range(n):
       leader=True
       for j in range(i+1,n):
         if(arr[i]<arr[j]):
            leader=False
            break
       if leader:
         result.append(arr[i])
   return result
arr=[4,7,2,1,0]
obj=Solution()
print(obj.lead(arr))
