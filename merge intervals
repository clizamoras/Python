class Solution:
  def merge(self,intervals):
     intervals.sort()
     n=len(intervals)
     ans=[]
     i=0
     while i<n:
       start=intervals[i][0]
       end=intervals[i][1]
       j=j+1
       while j<n and intervals[j][0]<=end:
          end=max(end,interval[j][1])
          j=j+1
       i=j
       ans.append([start,end])
      return ans
intervals=[[1,3,[2,6],[8,10]]
obj=Solution()
print(obj.merge(intervals))
