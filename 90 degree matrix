
class Solution:
  def matrices(self,matrix):
     rows=len(matrix)
     cols=len(matrix[0])
     for i in range(rows):
       for j in range(i,cols):
          matrix[i][j],matrix[j][i]=matrix[j][i],matrix[i][j]
     for i range(rows):
        matrix[i].reverse()
     return matrix
matrix=[[1,2,3],[4,5,6],[7,8,9]]
obj=Solution()
print(obj.matrices(matrix))
