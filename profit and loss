
class Solution:
  def bus(self,arr):
    n=len(arr)
    max_profit=0
    for i in range(n):
      for j in range(i+1,n):
         if(arr[j]-arr[i]>0)
            profit=arr[j]-arr[i]
            if(profit>0):
              max_profit=profit
    return max_profit
arr=[7,1,5,3,6,4]
obj=Solution()
print(obj.bus(arr))
