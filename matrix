class Solution:
  def zeros(self,matrix):
     rows=len(matrix)
     cols=len(matrix[0])
     set=[]
     for i in range(len(matrix)):
       for j in range(len(matrix[i]):
          if (matrix[i][j]==0):
             set.append((i,j))
     for i ,j in set:
       for k in rows:
         matrix[k][j]=0
       for l in cols:
         matrix[i][l]=0
    return matrix
matrix=[[1,1,1],[1,0,1],[1,1,1]]
obj=Solution()
print(obj.zeros(matrix))
